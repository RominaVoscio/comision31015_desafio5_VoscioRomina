<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
     <!-- Bootstrap CSS -->
     <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <title>Document</title>
</head>
<body>
    <div class="card">
        <div class="card-header">
            <h1 class="text-primary">Vista de Productos</h1>
        </div>
    <div class="card-body"> 
    <table class="table table-dark table-striped">
        <tr class="table-dark" >
            <th>ID</th>
            <th>Name</th>
            <th>Price</th>
            <th>Imagen</th>
        </tr>
        <% data.forEach(element => { %>
            <tr>
                <td class="table-dark"><%= element.id %></td>
                <td class="table-dark"><%= element.name %></td>
                <td class="table-dark"><%= element.price %></td>
                <td class="table-dark"> <img src="<%= element.image%>" class="img-fluid img-thumbnail" alt="..."></td>
            </tr>
        <% }); %>
    </table>
    <a class="btn btn-info" href="/" role="button">To Form</a>
    </div>
    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.9.2/dist/umd/popper.min.js" integrity="sha384-IQsoLXl5PILFhosVNubq5LC7Qb9DXgDA9i+tQ8Zj3iwWAwPtgFTxbJ8NT4GN1R8p" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.min.js" integrity="sha384-cVKIPhGWiC2Al4u+LWgxfKTRIcfu0JTxR+EQDz/bgldoEyl4H0zUF0QKbrJ0EcQF" crossorigin="anonymous"></script>
</body>
</html>